package  {		import flash.display.Sprite		public class Branch extends Sprite {				public var lastX:Number; // x pos of last growth on branch		public var lastY:Number; // y pos of last growth on branch		public var nodesArray:Array(); // store nodes on branch		public function Branch(startX:Number, startY:Number) {						lastX = startX;			lastY = startY;						nodesArray = new Array();			node = new Node(lastX, lastY);									// make an initial node			nodesArray.push(node);			addChild(node);					}	}	}