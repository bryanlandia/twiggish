package  {		import flash.display.Sprite;	import woollyyyy.utils.*;		internal final class NodeDisc extends Sprite {				public function NodeDisc(startX:Number, startY:Number) {			// draw an ellipse			with (graphics) {				beginFill(0x333333, 0.5);				ellWidth = randomRange(5,8);				ellHeight = randomRange(5,8);				drawEllipse(startX-ellWidth/2, startY-ellWidth/2, ellWidth, ellHeight);				endFill();			}		}			}	}