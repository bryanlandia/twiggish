package  {		import flash.display.Sprite;	import woollyyyy.utils.*;	import flash.events.Event;		internal final class TwigSection extends Sprite {				public var startX:Number;		public var startY:Number;				public function TwigSection(startX:Number, startY:Number) {			trace('making a new TwigSection');			this.startX = startX;			this.startY = startY;			_init();		}				private function _init(e:Event = null):void {						// draw a line			with (this.graphics) {				lineStyle(1, 0x333333); // TODO change color?				moveTo(this.startX, this.startY);				lineTo(randomRange(-50, 50) + this.startX,  // TODO eventually make this 					   randomRange(-50, 50) + this.startY); // curved and interesting			}		}			}	}